{
  "name": "yoso",
  "version": "1.1.1",
  "description": "",
  "bin": {
    "yoso": "bin/yoso.js"
  },
  "scripts": {
    "build": "tsc",
    "start:cli": "ts-node bin/yoso.ts",
    "test": "jest --config test/jest-config.json",
    "prepublish": "npm run build",
    "postpublish": "npm run build:clear",
    "build:clear": "find ./actions ./bin ./commands ./utils ./ui ./component -type f -name '*.d.ts' -delete & find ./actions ./bin ./commands ./utils ./ui ./component -type f -not -name '*.ts*' -delete",
    "release": "standard-version",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Linjovi/yoso.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Linjovi/yoso/issues"
  },
  "homepage": "https://github.com/Linjovi/yoso#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "commander": "^2.20.0",
    "final-form": "^4.16.1",
    "fs-finder": "^1.8.1",
    "handlebars": "^4.1.2",
    "ini": "^1.3.5",
    "ink": "^2.2.0",
    "ink-table": "^2.0.1",
    "ink-text-input": "^3.2.0",
    "inquirer": "^6.3.1",
    "log-symbols": "^3.0.0",
    "ora": "^3.4.0",
    "progress": "^2.0.3",
    "react": "^16.8.6",
    "react-final-form": "^6.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/commander": "^2.12.2",
    "@types/fs-finder": "^1.8.29",
    "@types/ini": "^1.3.30",
    "@types/ink-table": "^1.0.0",
    "@types/inquirer": "^6.0.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "@types/progress": "^2.0.3",
    "@types/react": "^16.8.22",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^24.8.0",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
